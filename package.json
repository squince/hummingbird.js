{
  "name": "hummingbird",
  "description": "Misspellings and nicknames are no match for this fast and forgiving typeahead engine.",
  "scripts": {
    "start": "webpack-dev-server --open",
    "build": "webpack --mode=production",
    "build.dev": "webpack --mode=development",
    "test": "webpack --mode=development && ./node_modules/mocha/bin/mocha ./tests/*.js"
  },
  "homepage": "https://github.com/squince/hummingbird.js",
  "version": "2.2.0",
  "index_version": "5.0",
  "author": "Daniel Griffis, Steve Quince, Amit Segal",
  "main": "hummingbird.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/squince/hummingbird.js.git"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "coffee-script": "1.3.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "4.2.3",
    "lint-staged": "10.1.5",
    "mocha": "8.1.3",
    "prettier": "2.0.2",
    "prompt": "0.2.12",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "diacritics": "1.3.0"
  },
  "engines": {
    "node": "13.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix"
    ],
    "*.{json}": [
      "eslint --fix"
    ]
  }
}
