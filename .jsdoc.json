{
  "opts": {
    "destination": "./api",
    "package": "./package.json",
    "readme": "./docs/readme.md"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "recurseDepth": 1,
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.mjs$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
